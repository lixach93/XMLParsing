<?xml version="1.0" encoding="UTF-8"?>
<flowers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://www.example.com/flowers"
         xsi:schemaLocation="http://www.example.com/flowers /orangery.xsd">


    <monocots-flower id="a1">
        <name>Chlorophytum</name>
        <origin>South Africa</origin>
        <visual>
            <leaf-color>Green</leaf-color>
            <stem-color>Green</stem-color>
            <length>50</length>
        </visual>
        <growing-tips>
            <temperature>26</temperature>
            <lighting>true</lighting>
            <watering>250</watering>
        </growing-tips>
        <multiplying>seed</multiplying>
        <firstMention>2000-10-20</firstMention>
    </monocots-flower>

    <monocots-flower id="a2">
        <name>Snowdrop</name>
        <origin>Europe</origin>
        <visual>
            <leaf-color>White</leaf-color>
            <stem-color>Dark green</stem-color>
            <length>15</length>
        </visual>
        <growing-tips>
            <temperature>-8</temperature>
            <lighting>false</lighting>
            <watering>100</watering>
        </growing-tips>
        <multiplying>leaf</multiplying>
        <firstMention>2000-10-20</firstMention>
    </monocots-flower>


</flowers>